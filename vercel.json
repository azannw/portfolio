{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "buildCommand": "",
  "installCommand": "",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/blog",
      "has": [
        {
          "type": "query",
          "key": "slug",
          "value": "(?<slug>.*)"
        }
      ],
      "destination": "/api/blog?slug=:slug"
    }
  ]
}
